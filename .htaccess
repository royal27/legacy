<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /

    # Rule to handle existing files and folders (like CSS, JS, images)
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d

    # Rule to redirect all other requests to index.php
    RewriteRule ^(.*)$ index.php?route=$1 [L,QSA]
</IfModule>
